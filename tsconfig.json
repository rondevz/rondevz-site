{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*", "src/types/*.d.ts", "src/@types/*.d.ts"],
	"exclude": ["dist"],
	"compilerOptions": {
		"strictNullChecks": true,
		"jsx": "react-jsx",
		"types": ["three"],
		"typeRoots": ["./node_modules/@types", "./src/types", "./src/@types"]
	}
}
